  <script>

    let sidebarCollapsed = `{{sidebarCollapsed}}`;

    const toggleSidebar = function (firstCall) {
      $('aside.navbar').toggleClass("d-lg-none");
      $('.sidebar-toggler').toggleClass("collapsed");
      $('.collapsed-sidebar').toggleClass("collapsed");
      $('.page-wrapper').toggleClass("ml-collapsed-sidebar");

      if (firstCall == true) return;

      const data = {
        sidebarCollapsed: !($('.collapsed-sidebar').hasClass('collapsed'))
      };

      $.ajax({
        url: `/toggleSidebar`,
        method: 'POST',
        data: JSON.stringify(data),
        contentType: 'application/json',
        success: (response) => {
          return;
        },
        error: (error) => {
          alert(error.response);
        }
      })
    }
  </script>
